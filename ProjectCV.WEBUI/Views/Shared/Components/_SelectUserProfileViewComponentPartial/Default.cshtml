@model UserProfile
<div class="profile-img">
    <img src="~/assets/img/@Model.ProfileImage" alt="" class="img-fluid rounded-circle">
</div>

<a href="index.html" class="logo d-flex align-items-center justify-content-center">
    <!-- Uncomment the line below if you also wish to use an image logo -->
    <!-- <img src="~/assets/img/logo.png" alt=""> -->
    <h1 class="sitename">@Model.Name @Model.Surname </h1>
</a>
<div class="social-links text-center">
    @foreach (var item in @Model.SocialMedias)
    {
        <a target="_blank" href="@item.Url" ><i class="@item.Icon"></i></a>
    }
    @* <a href="#" class="twitter"><i class="bi bi-twitter-x"></i></a> *@
</div>