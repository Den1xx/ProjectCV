@model List<Experiance>

<h3 class="resume-title">Professional Experience</h3>
@foreach(var item in Model)
{
    <div class="resume-item">
    <h4>@item.Title</h4>
    <h5>@item.StartDate.ToString("yyyy") - @item.EndDate.ToString("yyyy")</h5>
    <p><em>@item.CompanyName </em></p>
    <ul>
        <li>@item.Text</li>
    </ul>
</div>
}
